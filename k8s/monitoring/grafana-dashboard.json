{
  "annotations": {"list": []},
  "panels": [
    {
      "type": "graph",
      "title": "Pod CPU Usage",
      "targets": [{"expr": "sum(rate(container_cpu_usage_seconds_total{namespace=\"default\"}[5m])) by (pod)", "legendFormat": "{{pod}}"}],
      "datasource": "Prometheus"
    },
    {
      "type": "graph",
      "title": "Pod Memory Usage",
      "targets": [{"expr": "sum(container_memory_usage_bytes{namespace=\"default\"}) by (pod)", "legendFormat": "{{pod}}"}],
      "datasource": "Prometheus"
    },
    {
      "type": "graph",
      "title": "Todo Request Count",
      "targets": [{"expr": "todo_count", "legendFormat": "Todos"}],
      "datasource": "Prometheus"
    }
  ],
  "schemaVersion": 16,
  "title": "Kube TODO Monitoring",
  "version": 1
}
